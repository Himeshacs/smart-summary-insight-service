
# Application

NODE_ENV=development
PORT=3000
LOG_LEVEL=info
PUBLIC_BASE_URL=http://localhost:3000


# AI Provider Routing

AI_PROVIDERS_ENABLED=claude,openai,deepseek
AI_PROVIDER_STRATEGY=cost_then_failover  
AI_RL_COOLDOWN_MS=60000                 
AI_ERR_COOLDOWN_MS=15000                 


# Claude 

CLAUDE_API_KEY=claude key
CLAUDE_MODEL=claude-3-haiku-20240307
CLAUDE_MAX_TOKENS=1000
CLAUDE_TIMEOUT=30000
CLAUDE_BASE_URL=https://api.anthropic.com
CLAUDE_ANTHROPIC_VERSION=2023-06-01


CLAUDE_COST_PER_1K=0.00100

# OpenAI
OPENAI_API_KEY=openai_key
OPENAI_MODEL=gpt-4o-mini
OPENAI_TIMEOUT=30000
OPENAI_BASE_URL=https://api.openai.com/v1

OPENAI_COST_PER_1K=0.00025


# DeepSeek

DEEPSEEK_API_KEY=Deepseek_key
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TIMEOUT=30000
DEEPSEEK_BASE_URL=https://api.deepseek.com

DEEPSEEK_COST_PER_1K=0.00027

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
